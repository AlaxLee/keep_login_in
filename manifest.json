{
    "name": "Keep Login In",
    "description": "保持网站登录",
    "version": "1.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "popup.png"
    },
    "background": {
        "scripts": ["background.js","keeploginin.js"]
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"
    ]
}